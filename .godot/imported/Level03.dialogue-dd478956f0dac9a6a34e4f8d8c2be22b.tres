},
"12": {
"character": "AI",
"character_replacements": Array[Dictionary]([]),
"next_id": "13",
"text": "It should be fine, your suit has a built-in exoskeleton so you can still reach the bridge, even with increased gravity. [next=2]",
"text_replacements": Array[Dictionary]([]),
"translation_key": "It should be fine, your suit has a built-in exoskeleton so you can still reach the bridge, even with increased gravity. [next=2]",
