"10": {
"character": "AI",
"character_replacements": Array[Dictionary]([]),
"next_id": "11",
"text": "They won't work anymore with repairs. [next=2]",
"text_replacements": Array[Dictionary]([]),
"translation_key": "They won't work anymore with repairs. [next=2]",
