},
"11": {
"character": "AI",
"character_replacements": Array[Dictionary]([]),
"next_id": "12",
"text": "Hmmm...[wait=2] It also appears that gravity has increased as you get closer to the bridge. [next=2]",
"text_replacements": Array[Dictionary]([]),
"translation_key": "Hmmm...[wait=2] It also appears that gravity has increased as you get closer to the bridge. [next=2]",
