config_version=5

[application]

config/name="Time Aversion"
run/main_scene="res://src/scenes/interface/MainMenu.tscn"
config/features=PackedStringArray("4.0")
config/icon="res://icon.png"

[autoload]

GameStates="*res://src/autoloads/Game_States.gd"
PlayerVariables="*res://src/autoloads/PlayerVariables.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
MusicController="*res://src/autoloads/MusicController.tscn"
Save="*res://src/save/saveSettings.gd"
GlobalSettings="*res://src/scripts/global_settings.gd"
LevelManager="*res://src/autoloads/LevelManager.tscn"

[dialogue_manager]

general/states=["GameStates", "LevelManager"]

[display]

window/stretch/mode="2d"

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

